   <RecipeDef ParentName="SurgeryFlesh">
      <defName>InstallBrainStimulator</defName>
      <label>Install Brain Stimulator</label>
      <description>Installs a Brain Stimulator.</description>
      <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
      <jobString>Installing Brain Stimulator.</jobString>
      <workAmount>3600</workAmount>
      <ingredients>
         <li>
            <filter>
               <categories>
                  <li>Medicine</li>
               </categories>
            </filter>
            <count>1</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>BrainStimulator</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <categories>
            <li>Medicine</li>
         </categories>
         <thingDefs>
            <li>BrainStimulator</li>
         </thingDefs>
      </fixedIngredientFilter>
      <appliedOnFixedBodyParts>
         <li>Brain</li>
      </appliedOnFixedBodyParts>
      <addsHediff>BrainStimulator</addsHediff>
      <skillRequirements>
         <li>
            <skill>Medicine</skill>
            <minLevel>6</minLevel>
         </li>
      </skillRequirements>
        <researchPrerequisite>SK_MedicineIII</researchPrerequisite>
   </RecipeDef>
   
   
