		<soundDrop>Metal_Drop</soundDrop>
		<stackLimit>75</stackLimit>
		<statBases>
			<MarketValue>50</MarketValue>
			<DeteriorationRate>0</DeteriorationRate>
    		<Flammability>0</Flammability>
		</statBases>
		<thingCategories>
			<li>RARBar</li>
		</thingCategories>
	</ThingDef>

