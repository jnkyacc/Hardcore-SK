   <ThingDef ParentName="ResBase">
    <defName>cookie</defName>
    <label>Cookie</label>
    <description>Freshly baked cookies!</description>
    <graphicData>
      <texPath>Things/Item/cookie</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
      </graphicData>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>10</DeteriorationRate>
      <MarketValue>32</MarketValue>
      <WorkToMake>600</WorkToMake>
    </statBases>
    <tickerType>Rare</tickerType>
    <socialPropernessMatters>true</socialPropernessMatters>
   <comps>
      <li Class="CompProperties_Rottable">
         
        <daysToRotStart>4</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
		<thingCategories>
			<li>SweetMeals</li>
		</thingCategories>
    <ingestible>
	  <foodType>Processed</foodType>
      <maxNumToIngestAtOnce>1</maxNumToIngestAtOnce>
      <preferability>MealSimple</preferability>
      <nutrition>0.1</nutrition>
      <joy>0.3</joy>
      <eatEffect>EatVegetarian</eatEffect>
      <soundEat>Meal_Eat</soundEat>
    </ingestible>
  </ThingDef>

