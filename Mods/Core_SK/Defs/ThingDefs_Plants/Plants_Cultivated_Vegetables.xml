<?xml version="1.0" encoding="utf-8" ?>
<ThingDefs>

   <ThingDef ParentName="PlantBase">
    <defName>PlantPotato</defName>
    <label>Potato Plant</label>
    <description>Potato Plants produce nutritious tubers called Potatoes. Potato Plants grow quickly and are a reliable food source crop for basic meals.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>1</Beauty>
    </statBases>
  <graphicData>
      <texPath>Things/Plant/PotatoPlant</texPath>
  <graphicClass>Graphic_Random</graphicClass>
  </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <ingestible>
      <foodType>Plant</foodType>
      <preferability>NeverForNutrition</preferability>
      <nutrition>0.20</nutrition>
    </ingestible>
    <plant>
      <sowMinSkill>1</sowMinSkill>
      <dieIfLeafless>true</dieIfLeafless>
      <shootsSeeds>false</shootsSeeds>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>RawPotatoes</harvestedThingDef>
      <harvestDestroys>true</harvestDestroys>
      <harvestYield>9</harvestYield>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
      <topWindExposure>0.1</topWindExposure>
      <growDays>6.5</growDays>
      <fertilityFactorGrowthRate>0.4</fertilityFactorGrowthRate>
      <fertilityMin>0.5</fertilityMin>
      <visualSizeRange>
        <min>0.3</min>
        <max>1.05</max>
      </visualSizeRange>
    </plant>
  </ThingDef>
  
  
