   <RecipeDef ParentName="RecipeAdvancedProsthesis">
      <defName>CreateAdvancedBionicEye</defName>
      <label>Upgrade Bionic Eye</label>
      <description>Upgrades a bionic eye with biosynthetic material. Produces 1.</description>
      <jobString>Upgrading bionic eye.</jobString>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>Hyperweave</li>
               </thingDefs>
            </filter>
            <count>15</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>BiosyntheticMaterial</li>
               </thingDefs>
            </filter>
            <count>5</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>BionicEye</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <thingDefs>
            <li>Hyperweave</li>
            <li>BiosyntheticMaterial</li>
            <li>BionicEye</li>
         </thingDefs>
      </fixedIngredientFilter>
      <products>
         <AdvancedBionicEye>1</AdvancedBionicEye>
      </products>
   </RecipeDef>
   
   
