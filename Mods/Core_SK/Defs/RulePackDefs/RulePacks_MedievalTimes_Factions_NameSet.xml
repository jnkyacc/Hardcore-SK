<NameMakerDefs>


 <!-- ==================== Medieval Standard Faction ================= -->
  <RulePackDef>
    <defName>MedievalTimes_Faction_NameSet</defName>
    <rulePack>
      <rulesStrings>
	<li>name->House [outlander]</li>
	<li>name->Clan [outlander]</li>
	<li>name->[outlander]'s Kingdom</li>
	<li>name->[outlander] Estates</li>
	<li>name->[outlander]'s Royal Court</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>outlander</keyword>
          <pathList>
            <li>NameBanks/Nouns_People_Outlanders</li>
            <li>NameBanks/Nouns_People_Family</li>
          </pathList>
        </li>
      </rulesRaw>
    </rulePack>
	
	
 <!-- ==================== Medieval Raider Faction ================= -->	
  </RulePackDef>
    <RulePackDef>
    <defName>MedievalTimes_Raider_Faction_NameSet</defName>
    <rulePack>
      <rulesStrings>
			<li>name->[badassperson]'s [groupname]</li>
			<li>name->The [badasscolor][terrainfeature]  [groupname]</li>			
			<li>name->[badassperson] [terrainfeature]</li>	
	
      </rulesStrings>
      <rulesRaw>
	  
        <li Class="Rule_File">
          <keyword>badassperson</keyword>
          <path>NameBanks/Nouns_BadassPersons</path>
        </li>
        <li Class="Rule_File">
          <keyword>groupname</keyword>
          <path>NameBanks/Nouns_GroupNames</path>
        </li>		
        <li Class="Rule_File">
          <keyword>badasscolor</keyword>
          <path>NameBanks/ColorsBadass</path>
        </li>
        <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>NameBanks/Nouns_TerrainFeatures</path>
        </li>		
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
  
</NameMakerDefs>
