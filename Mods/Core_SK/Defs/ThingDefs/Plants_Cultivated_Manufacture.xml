<ThingDefs>

    <ThingDef ParentName="PlantBase">
    <defName>PlantCotton</defName>
    <label>Cotton Bush</label>
    <description>Cotton Bushes produce Cotton, a fine plant fiber that can be spun from the Tailor's Workbench into a textile called Cloth.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>1</Beauty>
    </statBases>
  <graphicData>
	<texPath>Things/Plant/CottonPlant</texPath>
  <graphicClass>Graphic_Random</graphicClass>
  </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <ingestible>
      <foodType>Plant</foodType>
      <preferability>NeverForNutrition</preferability>
      <nutrition>0.01</nutrition>
    </ingestible>
    <plant>
      <sowMinSkill>5</sowMinSkill>
      <sowResearchPrerequisites>
        <li>SK_FiberCropsI</li>
      </sowResearchPrerequisites>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
      <dieIfLeafless>true</dieIfLeafless>
      <shootsSeeds>false</shootsSeeds>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>RawCotton</harvestedThingDef>
      <harvestDestroys>true</harvestDestroys>
      <harvestYield>5</harvestYield>
      <topWindExposure>0.1</topWindExposure>
      <growDays>5.41</growDays>
      <fertilityFactorGrowthRate>1</fertilityFactorGrowthRate>
      <fertilityMin>0.5</fertilityMin>
      <visualSizeRange>
        <min>0.3</min>
        <max>1.05</max>
      </visualSizeRange>
    </plant>
  </ThingDef>
  
    
