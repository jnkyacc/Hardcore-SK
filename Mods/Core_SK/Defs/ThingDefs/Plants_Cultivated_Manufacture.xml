  <ThingDef ParentName="PlantBase">
    <defName>WeedPlant</defName>
    <label>Weed Plant</label>
    <description>Weed Plants produce small, psychoactive buds called Weed. Weed is rolled from the Tobacco Rolling Bench and smoked as a type of addictive drug. It can also be processed into a textile called Hemp made from the Tailor's Workbench.</description>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Beauty>3</Beauty>
    </statBases>
	<graphicData>
    <texPath>Things/Plant/Weedplant</texPath>
    <graphicClass>Graphic_Single</graphicClass>
	</graphicData>
    <selectable>true</selectable>
    <pathCost>30</pathCost>
    <ingestible>
      <foodType>Plant</foodType>
      <preferability>NeverForNutrition</preferability>
      	<nutrition>0.05</nutrition>
	<joy>0.05</joy>
	<joyKind>Chemical</joyKind>
    </ingestible>
    <plant>
      <sowMinSkill>6</sowMinSkill>
      <sowResearchPrerequisites>
        <li>SK_DrugCropsII</li>
      </sowResearchPrerequisites>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
	  <dieIfLeafless>true</dieIfLeafless>
      <harvestWork>200</harvestWork>
      <harvestTag>Standard</harvestTag>
      <harvestDestroys>true</harvestDestroys>
      <harvestedThingDef>Weed</harvestedThingDef>
      <harvestYield>2</harvestYield>
	  <growDays>10.52</growDays>
